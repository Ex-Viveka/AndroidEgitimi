<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.kanilturgut.notificationdenemeleri"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.VIBRATE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".NormalNotificaitonActivity">
        </activity>
        <activity android:name=".ActionNotificationActivity">
        </activity>
        <activity android:name=".FirstActionNotificationActivity">
        </activity>
        <activity android:name=".SecondActionNotificationActivity">
        </activity>
    </application>

</manifest>
